{
    "name": "hiddenalt/bot_memi4",
    "description": "A cross-platform bot focused on generating random memes",
    "keywords": [
        "bot",
        "chat-bot",
        "meme",
        "memes",
        "telegram",
        "vk",
        "vkontakte"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "botman/botman": "~2.0",
        "botman/driver-telegram": "^1.5",
        "botman/driver-web": "~1.0",
        "botman/studio-addons": "~1.3",
        "botman/tinker": "~1.0",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.5",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "league/flysystem": "^1.0",
        "nuwave/lighthouse": "^4.16",
        "opis/closure": "^3.4.2",
        "ralouphie/mimey": "^2.1",
        "symfony/string": "^5.1",
        "yageorgiy/botman-vk-community-callback-driver": "^1.6.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "BotMan\\Studio\\Providers\\DriverServiceProvider::publishDriverConfigurations"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "BotMan\\Studio\\Providers\\DriverServiceProvider::publishDriverConfigurations"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
